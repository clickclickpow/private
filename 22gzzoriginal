    [1] = "God Breaker",
    [2] = 0,
    [3] = -2000,
    [4] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [5] = game:GetService("Players").LocalPlayer.Backpack["God Breaker"]
}

game:GetService("ReplicatedStorage").Events.godbreaker:FireServer(unpack(args))

   end
end)

