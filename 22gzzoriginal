hy = t
   while hy do task.wait()
      local args = {
    [1] = "God Breaker"
}

game:GetService("ReplicatedStorage").Events.giveskill:FireServer(unpack(args))


local args = {
    [1] = "Final Explosion"
}

game:GetService("ReplicatedStorage").Events.giveskill:FireServer(unpack(args))
wait(0.1)


